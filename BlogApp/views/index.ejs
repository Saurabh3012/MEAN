<% include ./partials/header.ejs %>


<h1>Index page</h1>

<% blogs.forEach(function(blog){ %>
	<div>
		<h2><%= blog.title%></h2>
		<img src="<%= blog.image %>">
		<p><%= blog.created %></p>
		<p><%= blog.body%></p>
		<a href="/blogs/<%= blog._id %>">read more ...</a>
	</div>

<% }); %>

<%  include ./partials/footer.ejs %>